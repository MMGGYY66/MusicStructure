<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".NowPlaying">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ArtistActivity"
            android:label="@string/artistActivity" />
        <activity
            android:name=".EqualizerActivity"
            android:label="@string/equalizerActivity" />
        <activity
            android:name=".MusicLibraryActivity"
            android:label="@string/musicLibraryActivity" />
        <activity
            android:name=".LyricsActivity"
            android:label="@string/lyricsActivity" />
        <activity android:name=".PlaylistActivity"
            android:label="@string/playlistActivity"></activity>
    </application>

</manifest>